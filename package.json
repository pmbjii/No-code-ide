    "preview": "vite preview",
    "start": "node server/index.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@monaco-editor/react": "^4.6.0",
    "monaco-editor": "^0.44.0",
    "axios": "^1.6.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "ws": "^8.14.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "node-pty": "^1.0.0",
    "chokidar": "^3.5.3",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "marked": "^9.1.6",
    "prismjs": "^1.29.0",
    "react-hotkeys-hook": "^4.4.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "lucide-react": "^0.294.0",
    "clsx": "^2.0.0",
    "tailwindcss": "^3.3.5",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.31"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.1.1",
    "vite": "^4.5.0",
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2",
    "typescript": "^5.2.2"
  },
  "keywords": ["ai", "code-editor", "cursor", "ide", "react", "monaco"],
