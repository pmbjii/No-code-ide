{
  "name": "cursor-ai-clone",
  "version": "1.0.0",
  "description": "Advanced AI-powered code editor with custom API support",
  "main": "src/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
